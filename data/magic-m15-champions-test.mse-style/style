	image 2:
		left: {if comma_count(styling.popout_image_style) == ",,,," then split_text(match:",", styling.popout_image_style).0 else "0"}
		top: {if comma_count(styling.popout_image_style) == ",,,," then split_text(match:",", styling.popout_image_style).1 else "0"}
		width: {if comma_count(styling.popout_image_style) == ",,,," then split_text(match:",", styling.popout_image_style).2 else "0"}
		height: {if comma_count(styling.popout_image_style) == ",,,," then split_text(match:",", styling.popout_image_style).3 else "0"}
		z index: 9
	############################# Card type
	indicator:
		left: 31
		top: { if mainframe_walker() then 317 else 332 }
		width: 17
		height: 17
		z index: 5
		render style: image
		visible: { has_identity() }
		image: { card_identity() }
	type:
		left: { if has_identity() then "52" else "32" }
		top: { (if mainframe_walker() then 314 else if is_blank_token() then 444 else if styling.fullart then 430 else 329) + shrink_type() }
		width: { (if has_identity() then "290" else "310") - max(22,card_style.rarity.content_width) }
		height: { 20 - shrink_type()}
